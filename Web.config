<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="false" />
    <httpHandlers>
      <add path="download" verb="POST" type="ZoomCharts.ProxyEcho.ProxyHandler, ZoomCharts.ProxyEcho"/>
    </httpHandlers>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
