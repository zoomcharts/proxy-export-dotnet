    <defaultDocument>
      <files>
        <clear/>
        <add value="index.html"/>
      </files>
    </defaultDocument>
    <handlers>
      <add name="ProxyHandler" path="download" verb="POST" type="ZoomCharts.ProxyEcho.ProxyHandler, ZoomCharts.ProxyEcho"/>
    </handlers>
  </system.webServer>
</configuration>